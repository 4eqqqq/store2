          "_name": "features2",
          "_customHTML": "<section class=\"features2\" group=\"Animated Features\" data-bg-video=\"{{bg.type == 'video' && bg.value.url}}\" mbr-class=\"{'mbr-parallax-background': bg.parallax}\">\r\n\r\n    <mbr-parameters>\r\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\r\n    <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"9\" step=\"1\" value=\"6\">\r\n    <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"9\" step=\"1\" value=\"6\">\r\n    <input type=\"checkbox\" title=\"Show Title\" name=\"showTitle\" checked>\r\n    <input type=\"checkbox\" title=\"Show Subtitle\" name=\"showSubtitle\" checked>\r\n    <input type=\"checkbox\" title=\"Show Text\" name=\"showText\" checked>\r\n    <input type=\"color\" title=\"Card Color\" name=\"cardColor\" value=\"#efefef\">\r\n    <select title=\"Cards\" name=\"cardsAmount\">\r\n        <option value=\"1\">1</option>\r\n        <option value=\"2\">2</option>\r\n        <option value=\"3\" selected>3</option>\r\n    </select>\r\n        <fieldset type=\"background\" name=\"bg\" parallax>\r\n            <input type=\"image\" title=\"Background Image\" value=\"../_images/background7.jpg\" parallax>\r\n            <input type=\"color\" title=\"Background Color\" value=\"#ffffff\" selected>\r\n            <input type=\"video\" title=\"Background Video\" value=\"https://www.youtube.com/watch?v=kI1YE_qenSU\">\r\n        </fieldset>\r\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\">\r\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#efefef\" condition=\"overlay && bg.type !== 'color'\">\r\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.8\" condition=\"overlay && bg.type !== 'color'\">\r\n    <!-- End block parameters -->\r\n    </mbr-parameters>\r\n\r\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\r\n    </div>\r\n    <div class=\"container\">\r\n        <h2 class=\"mbr-fonts-style mbr-section-title align-center\"\r\n            mbr-if=\"showTitle\"\r\n            mbr-theme-style=\"display-2\"\r\n            data-app-selector=\".mbr-section-title\">\r\n            FRESH &amp; LOCAL\r\n        </h2>\r\n        <h3 class=\"mbr-fonts-style mbr-section-subtitle align-center\"\r\n            mbr-if=\"showSubtitle\"\r\n            mbr-theme-style=\"display-5\"\r\n            data-app-selector=\".mbr-section-subtitle\">\r\n            explore our menus\r\n        </h3>\r\n        <div class=\"row justify-content-center food-content mt-5\">\r\n            <div class=\"col-sm-12 col-lg-4 content-section\">\r\n                <div class=\"image-wrap text-center\">\r\n                    <img src=\"../_images/img/delicious1.jpg\" alt>\r\n                    <div class=\"content-block\"\r\n                         mbr-if=\"showText\">\r\n                        <h4 class=\"mbr-fonts-style mbr-text align-center\"\r\n                            mbr-theme-style=\"display-2\"\r\n                            data-app-selector=\".mbr-text\">\r\n                            Dinner\r\n                        </h4>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n            <div class=\"col-sm-12 col-lg-4 content-section\"\r\n                 mbr-if=\"cardsAmount>1\">\r\n                <div class=\"image-wrap text-center\">\r\n                    <img src=\"../_images/img/delicious2.jpg\" alt>\r\n                    <div class=\"content-block\"\r\n                         mbr-if=\"showText\">\r\n                        <h4 class=\"mbr-fonts-style mbr-text align-center\"\r\n                            mbr-theme-style=\"display-2\"\r\n                            data-app-selector=\".mbr-text\">\r\n                            Lunch\r\n      